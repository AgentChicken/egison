(define $write-each
  (lambda [$xs]
    (match xs (list something)
      {[<nil> (do {} [])]
       [<cons $x $rs>
        (do {[(write x)]
             [(write "\n")]
             [(write-each rs)]
             })]})))

(define $main
  (lambda [$argv]
    (do {[(write "argv: ")]
         [(write (show argv))]
         [(write "\n")]
         [(write-each argv)]})))
