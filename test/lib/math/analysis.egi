(assert-equal "d/d - case 1"
  (d/d (** x 2) x)
  (* 2 x))

(assert-equal "d/d - case 2"
  (d/d (** a (** x 2)) x)
  (* 2 (** a x^2) (log a) x))

(assert-equal "d/d - case 3"
  (d/d (* (cos x) (sin x)) x)
  (+ (sin x)^2 (* -1 (cos x)^2)))

(assert-equal "d/d - case 4"
  (d/d (sigmoid z) z)
  (/ (exp (* -1 z)) (+ 1 (* 2 (exp (* -1 z))) (exp (* -1 z))^2)))

(assert-equal "d/d - case 5"
  (d/d (d/d (log x) x) x)
  (/ -1 x^2))
